<h1>Capital Buid Up</h1>

<%= link_to 'New Capital build up', new_capital_build_up_path %>
<br/><%= link_to 'Capital build up Payment', cbu_contributions_path %>
<br/><%= link_to 'Home', root_path %>
<br/>
<br/>
<table>
  <thead>
    <tr>
      <th>Memberid</th>
      <th>Membershiptype</th>
      <th>Membersince</th>
      <th>Subscribedshares</th>
     
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @capital_build_ups.each do |capital_build_up| %>
      <tr>
        <td><%= capital_build_up.memberId %></td>
        <td><%= capital_build_up.membershipType %></td>
        <td><%= capital_build_up.memberSince %></td>
        <td><%= capital_build_up.subscribedShares %></td>
        
        <td><%= link_to 'Show', capital_build_up %></td>
        <td><%= link_to 'Edit', edit_capital_build_up_path(capital_build_up) %></td>
        <td><%= link_to 'Destroy', capital_build_up, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

